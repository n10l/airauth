{
  "name": "@airauth/next",
  "version": "1.0.0-beta.0",
  "description": "Next.js integration for AirAuth - Modern authentication for Next.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./app": {
      "types": "./dist/app/index.d.ts",
      "import": "./dist/app/index.mjs",
      "require": "./dist/app/index.js"
    },
    "./pages": {
      "types": "./dist/pages/index.d.ts",
      "import": "./dist/pages/index.mjs",
      "require": "./dist/pages/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js",
      "require": "./dist/middleware/index.cjs"
    }
  },
  "files": [
    "dist/**",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "airauth",
    "authentication",
    "nextjs",
    "next",
    "auth",
    "oauth",
    "jwt",
    "session"
  ],
  "author": "n10l",
  "license": "MIT",
  "homepage": "https://airauth.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/n10l/airauth",
    "directory": "packages/next"
  },
  "bugs": {
    "url": "https://github.com/n10l/airauth/issues"
  },
  "peerDependencies": {
    "@airauth/core": "workspace:*",
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@airauth/core": "workspace:*",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "next": "^15.5.4",
    "react": "^19.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.0",
    "vitest": "^3.2.4"
  }
}
